{
  "name": "we-framework",
  "version": "2.0.0",
  "description": "Wikidata edit gadgets for using from Wikipedia projects",
  "main": "src/app.js",
  "browserslist": ">0.1%, safari >= 10, ie >= 11",
  "scripts": {
    "eslint:check": "eslint --ignore-pattern \"!.eslintrc.js\" .eslintrc.js deploy-to-wikipedia.js karma.conf.js webpack.config.js src/**/*.ts* test/.eslintrc.js test/**/*.ts*",
    "eslint:fix": "eslint --ignore-pattern \"!.eslintrc.js\" --fix .eslintrc.js deploy-to-wikipedia.js karma.conf.js webpack.config.js src/**/*.ts* test/.eslintrc.js test/**/*.ts*",
    "serve": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack && eslint ./deploy-to-wikipedia.js --fix",
    "deploy": "npm run build && node ./deploy-to-wikipedia.js",
    "test": "npm run eslint:check && npm run flow:check && npm run test:karma",
    "test:karma": "cross-env NODE_ENV=development karma start karma.conf.js --single-run",
    "test:watch": "cross-env NODE_ENV=development karma start karma.conf.js --no-single-run",
    "test:watch:chrome": "cross-env NODE_ENV=development karma start karma.conf.js --no-single-run --browsers Chrome"
  },
  "devDependencies": {
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@types/chai": "^4.3.0",
    "@types/md5": "^2.3.2",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-autosuggest": "^10.1.5",
    "@types/react-dom": "^17.0.13",
    "@types/react-redux": "^7.1.23",
    "@types/react-tagsinput": "^3.19.9",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vlsergey/batcher": "^1.0.0",
    "@vlsergey/js-config": "^10.0.0",
    "autoprefixer": "^9.8.0",
    "browserslist": "^4.16.5",
    "chai": "^4.3.6",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "emotion": "^10.0.27",
    "eslint-webpack-plugin": "^3.1.1",
    "fetch-jsonp": "^1.1.3",
    "fetch-mock": "^9.10.1",
    "fetch-polyfill": "^0.8.2",
    "form-data": "^3.0.0",
    "global-jsdom": "^6.1.0",
    "https-proxy-agent": "^5.0.0",
    "isbn": "git+https://github.com/AV-Forlaget/isbnjs.git#master",
    "isomorphic-fetch": "^2.2.1",
    "md5": "^2.3.0",
    "mini-css-extract-plugin": "^2.6.0",
    "node-fetch": "^2.6.7",
    "node-storage-shim": "^2.0.1",
    "postcss-loader": "^3.0.0",
    "react-autosuggest": "^10.0.2",
    "react-redux": "^7.2.0",
    "react-spinners": "^0.8.3",
    "react-tagsinput": "^3.19.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.5",
    "reselect": "^4.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.1.2",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^3.3.1",
    "tslib": "^2.3.1",
    "wikitext-dom": "^0.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vlsergey/WE-Framework.git"
  },
  "keywords": [
    "wikidata",
    "wikipedia"
  ],
  "author": "Sergey Vladimirov a.k.a. vlsergey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vlsergey/WE-Framework/issues"
  },
  "homepage": "https://github.com/vlsergey/WE-Framework#readme"
}
